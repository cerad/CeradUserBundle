{{ form_start(form) }}
<fieldset><legend><span>Change Password</span></legend>
  {{ form_row(form.token)    }}
  {{ form_row(form.password) }}
</fieldset>
<fieldset>
  <div class="controls">
    <span><a href="{{ path('cerad_user__welcome') }}">Return to Welcome.</a></span>
    {{ form_widget(form.reset) }}
  </div>
</fieldset>
{{ form_rest(form) }}
{{ form_end (form) }}
